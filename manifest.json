{
  "name": "No more traslating tweet",
  "description": "Hide \"Translate Tweet\" on Twitter",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "http://*.twitter.com/*",
        "https://*.twitter.com/*"
        ],
      "js":["content.js"]
    }
  ]
}
